//- контент с тектом рыбой
//- аргументы (количество слов, язык, по умолчанию 'la' - Латынь)
mixin lorem(wordCount, loremLang)
	-var text = ''
	-var lang = loremLang || 'la'
	//- в цикле набираем текст
	each word, i in LOREM[lang]
		if i === wordCount
			-break
		-text += ' ' + word
	//- возвращаем составленый текст
	= text
